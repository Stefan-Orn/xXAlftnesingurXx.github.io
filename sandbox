    <script>
    $(document).ready(function () {
        $.ajax('/showads.js')
            .done(function () {
                if (typeof gameId != "undefined") {
                    countAdImpression(gameId, 'banner', 2);
                }
                adBlockStatus(false);
            })
            .fail(function (d) {
                if (d.status === 0 || d.statusText == 'error') {
                    window.adBlockEnabled = true;
                    adBlockStatus(true);
                }
            });
    });
</script>
